{
  "name": "ColorfulChouseisan",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "調整さんの出欠アイコンを見やすいものに差し替えます",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://chouseisan.com/*", "https://chouseisan.com/*"],
      "js": ["jquery-3.6.0.min.js","script.js"],
      "run_at": "document_end"
    }
  ]
}
